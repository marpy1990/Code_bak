<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
    "http://getahead.org/dwr/dwr30.dtd">
<dwr>
	<allow>
		<create creator="static" javascript="DataServices">
			<param name="class" value="prediction.DataServices" />
		</create>
		<create creator="static" javascript="PredictionServices">
			<param name="class" value="prediction.PredictionServices" />
		</create>
		<convert converter="bean" match="prediction.DataServices"/>
		<convert converter="bean" match="prediction.PredictionServices"/>
	</allow>	
</dwr>
